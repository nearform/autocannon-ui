{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended",
    "plugin:prettier/recommended",
    "plugin:react-hooks/recommended",
    "plugin:mocha/recommended"
  ],
  "plugins": ["import"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "TextDecoderStream": true
  }
}
