{
  "extends": ["standard", "plugin:prettier/recommended"],
  "env": {
    "node": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "strict": ["error", "global"],
    "import/order": [
      "error",
      {
        "newlines-between": "always"
      }
    ]
  }
}
