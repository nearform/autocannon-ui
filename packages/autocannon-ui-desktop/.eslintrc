{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["import"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "rules": {
    "strict": ["error", "global"],
    "import/order": [
      "error",
      {
        "newlines-between": "always"
      }
    ]
  }
}
